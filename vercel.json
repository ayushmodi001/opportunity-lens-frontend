{
    "functions": {
      "src/app/api/**/*.{js,ts}": {
        "maxDuration": 30,
        "memory": 512
      },
      "src/app/api/auth/**/*.{js,ts}": {
        "maxDuration": 30,
        "memory": 512
      },
      "src/app/(auth|dashboard)/**/*.{js,ts}": {
        "maxDuration": 30,
        "memory": 512
      },
      "src/app/**/*.js": {
        "maxDuration": 30
      },
      "src/app/**/*.jsx": {
        "maxDuration": 30
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/app/api/$1"
      }
    ]
  }