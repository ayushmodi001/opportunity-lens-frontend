{
    "functions": {
      "src/app/api/**/*.{js,ts}": {
        "maxDuration": 30,
        "memory": 3008
      },
      "src/app/api/auth/**/*.{js,ts}": {
        "maxDuration": 30,
        "memory": 3008
      },
      "src/app/(auth|dashboard)/**/*.{js,ts}": {
        "maxDuration": 30,
        "memory": 3008
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/app/api/$1"
      }
    ],
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/nex"
      }
    ]
  }